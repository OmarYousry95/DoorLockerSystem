#include "lcd.h"
#include "keypad.h"


int main()
{
	/*Initialises LCD*/
	LCD_init();
	uint8 key;
	while(1)
	{
		key=KEYPAD_getPressedKey();
		LCD_intgerToString(key);
	}
}
