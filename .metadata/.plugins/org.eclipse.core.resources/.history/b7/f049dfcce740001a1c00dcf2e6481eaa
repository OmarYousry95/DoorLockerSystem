#include "HMI_ECU.h"


int main()
{
	/*Initialises LCD*/
	LCD_init();
	uint8 key;
	while(1)
	{
		key=KEYPAD_getPressedKey();
		LCD_intgerToString(key);
	}
}


void HMI_welcome(void)
{
	LCD_displayStringRowColumn(0,2,"Door Locker");
	LCD_displayStringRowColumn(1,5,"Welcome");
}
